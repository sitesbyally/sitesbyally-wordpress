            "post_type": "page",
            "post_title": "Contact",
            "post_content": "<!-- wp:paragraph {\"align\":\"left\"} --> <p class=\"has-text-align-left\">Laten we dit bespreken &amp;amp;#128075; aarzel niet om contact met ons op te nemen via de onderstaande contactgegevens, of een bericht te sturen via het formulier.</p> <!-- /wp:paragraph --> <!-- wp:spacer {\"height\":32,\"className\":\"desktop-only\"} --> <div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer desktop-only\"></div> <!-- /wp:spacer --> <!-- wp:heading {\"textAlign\":\"left\"} --> <h2 class=\"has-text-align-left\">Neem contact op</h2> <!-- /wp:heading --> <!-- wp:jetpack/contact-info --> <div class=\"wp-block-jetpack-contact-info\"><!-- wp:jetpack/address {\"address\":\"10 Street Road\",\"city\":\"City\",\"postal\":\"10100\",\"country\":\"USA\"} --> <div class=\"wp-block-jetpack-address\"><div class=\"jetpack-address__address jetpack-address__address1\">Straat 10</div><div><span class=\"jetpack-address__city\">1234 AB</span>, <span class=\"jetpack-address__region\"></span><span class=\"jetpack-address__postal\">Stad</span></div><div class=\"jetpack-address__country\">Nederland</div></div> <!-- /wp:jetpack/address --> <!-- wp:jetpack/email {\"email\":\"mail@example.com\"} --> <div class=\"wp-block-jetpack-email\"><a href=\"mailto:mail@example.com\">email@voorbeeld.com</a></div> <!-- /wp:jetpack/email --> <!-- wp:jetpack/phone {\"phone\":\"(555) 555 1234\"} --> <div class=\"wp-block-jetpack-phone\"><a href=\"tel:5555551234\">+31 12345678</a></div> <!-- /wp:jetpack/phone --></div> <!-- /wp:jetpack/contact-info --> <!-- wp:spacer {\"height\":32,\"className\":\"desktop-only\"} --> <div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer desktop-only\"></div> <!-- /wp:spacer --> <!-- wp:heading {\"textAlign\":\"left\"} --> <h2 class=\"has-text-align-left\">Stuur ons een bericht</h2> <!-- /wp:heading --> <!-- wp:jetpack/contact-form {\"subject\":\"[Blockbase] Contact\"} --> <!-- wp:jetpack/field-name {\"required\":true} 